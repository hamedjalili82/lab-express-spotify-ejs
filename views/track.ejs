<!DOCTYPE html>
<html>
<head>
  <title>Album Tracks</title>
</head>
<body>
  <h1>Album Tracks</h1>
  <% tracks.forEach(track => { %>
    <div>
      <h3><%= track.name %></h3>
      <% if (track.preview_url) { %>
        <audio controls>
          <source src="<%= track.preview_url %>" type="audio/mpeg">
          Your browser does not support the audio element.
        </audio>
      <% } else { %>
        <p>No preview available for this track</p>
      <% } %>
    </div>
  <% }) %>
</body>
</html>
